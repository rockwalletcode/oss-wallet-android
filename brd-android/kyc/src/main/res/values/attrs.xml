<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckedTextView">
        <attr name="contentText" format="string"/>
        <attr name="iconImage" format="reference"/>
    </declare-styleable>

    <declare-styleable name="PhotoFinderView">
        <attr name="type" format="enum">
            <enum name="selfie" value="0"/>
            <enum name="document" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="InfoView">
        <attr name="infoTitle" format="string" />
        <attr name="infoDescription" format="string" />
    </declare-styleable>
</resources>